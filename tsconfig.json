{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@middlewares/*": [
        "./src/common/middlewares/*"
      ],
      "@decorators/*": [
        "./src/common/decorators/*"
      ],
      "@filters/*": [
        "./src/common/filters/*"
      ],
      "@enums/*": [
        "./src/common/enums/*"
      ],
      "@interfaces/*": [
        "./src/common/interfaces/*"
      ],
      "@dtos/*": [
        "./src/account/dto/*",
        "./src/util/dto/*"
      ],
      "@services/*": [
        "./src/common/services/*",
        "./src/juvhost/*",
        "./src/account/*",
        "./src/util/*",
        "./src/*"
      ],
      "@providers/*":[
        "./src/redis/*",
      ],
      "@modules/*": [
        "./src/account/*",
        "./src/util/*",
        "./src/common/*",
        "./src/http/*",
        "./src/juvhost/*",
        "./src/*"
      ],
      "@controllers/*": [
        "./src/account/*",
        "./src/util/*",
      ],
      "@common/guards/*": [
        "./src/common/guards/*",
      ]
    }
  },
  "include": [
    "./src/*"
  ],
  "exclude": [
    "./node_modules/*",
    "./dist/*"
  ]
}
